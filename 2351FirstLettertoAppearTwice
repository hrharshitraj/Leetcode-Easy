class Solution {
    public char repeatedCharacter(String s) {
        HashSet<Character> hm = new HashSet<>();
        char ans = 'a';
        for(int i = 0; i < s.length(); i++) {
            if(hm.contains(s.charAt(i))) return s.charAt(i);
            hm.add(s.charAt(i));
        } 
        return 'a';
    }
}

